    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "standard": "^14.3.3"
  },
  "scripts": {
    "test": "mocha \"src/tests/*.test.js\"",
    "watch-test": "mocha --reporter=min --watch \"src/tests/*.test.js\"",
